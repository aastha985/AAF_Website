<ol class="breadcrumb">
    <% breadcrumb.slice(0,breadcrumb.length-1).forEach((a)=>{ %>
        <li class="breadcrumb-item"><a href=<%=a.url%>><%=a.label.slice(0,1).toUpperCase()+a.label.slice(1)%></a></li>
   <% });%>
   <li class="breadcrumb-item active " aria-current="page"><%=breadcrumb[breadcrumb.length-1].label.slice(0,1).toUpperCase()+breadcrumb[breadcrumb.length-1].label.slice(1,breadcrumb[breadcrumb.length-1].label.search("&id=")!=-1?breadcrumb[breadcrumb.length-1].label.search("&id="):breadcrumb[breadcrumb.length-1].length)%></li>
</ol>
<style>
    .breadcrumb{
        margin-bottom: 3rem;
        padding-left: 3.5rem;
    }
</style>